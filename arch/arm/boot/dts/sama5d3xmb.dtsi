#include "sama5d3xcm.dtsi"

/ {
	compatible = "atmel,sama5d3xmb", "atmel,sama5d3xcm", "atmel,sama5d3", "atmel,sama5";

	ahb {
		apb {
			mmc0: mmc@f0000000 {
				pinctrl-names = "default", "sleep";
				pinctrl-0 = <&pinctrl_mmc0_clk_cmd_dat0 &pinctrl_mmc0_dat1_3 &pinctrl_mmc0_cd>;
				pinctrl-1 = <&pinctrl_mmc0_clk_cmd_dat0_sleep &pinctrl_mmc0_dat1_3_sleep &pinctrl_mmc0_dat4_7_sleep>;
				status = "okay";
				slot@0 {
					reg = <0>;
					bus-width = <4>;
					cd-gpios = <&pioD 17 GPIO_ACTIVE_HIGH>;
				};
			};

			can0: can@f000c000 {
				pinctrl-names = "default", "sleep";
				pinctrl-1 = <&pinctrl_can0_rx_tx_sleep>;
			};

			can1: can@f8010000 {
				pinctrl-names = "default", "sleep";
				pinctrl-1 = <&pinctrl_can1_rx_tx_sleep>;
			};

			spi0: spi@f0004000 {
				pinctrl-names = "default", "sleep";
				pinctrl-1 = <&pinctrl_spi0_sleep>;
				m25p80@0 {
					compatible = "atmel,at25df321a";
					spi-max-frequency = <50000000>;
					reg = <0>;
				};
			};

			ssc0: ssc@f0008000 {
				pinctrl-names = "default", "sleep";
				pinctrl-1 = <&pinctrl_ssc0_tx_sleep &pinctrl_ssc0_rx_sleep>;
			};

			i2c1: i2c@f0018000 {
				wm8904: wm8904@1a {
					compatible = "wm8904";
					reg = <0x1a>;

					DCVDD-supply = <&wm8904_1v8_power>;
					DBVDD-supply = <&wm8904_3v3_power>;
					AVDD-supply = <&wm8904_1v8_power>;
					CPVDD-supply = <&wm8904_1v8_power>;
					MICVDD-supply = <&wm8904_3v3_power>;
				};
			};

			usart1: serial@f0020000 {
				pinctrl-names = "default", "sleep";
				pinctrl-0 = <&pinctrl_usart1 &pinctrl_usart1_rts_cts>;
				pinctrl-1 = <&pinctrl_usart1_sleep &pinctrl_usart1_rts_cts_sleep>;
				status = "okay";
			};

			isi: isi@f0034000 {
				pinctrl-names = "default", "sleep";
				pinctrl-0 = <&pinctrl_isi &pinctrl_isi_pck_as_mck &pinctrl_isi_power &pinctrl_isi_reset>;
				pinctrl-1 = <&pinctrl_isi_sleep &pinctrl_isi_pck_as_mck_sleep &pinctrl_isi_power_sleep &pinctrl_isi_reset_sleep>;
			};

			mmc1: mmc@f8000000 {
				pinctrl-names = "default", "sleep";
				pinctrl-0 = <&pinctrl_mmc1_clk_cmd_dat0 &pinctrl_mmc1_dat1_3 &pinctrl_mmc1_cd>;
				pinctrl-1 = <&pinctrl_mmc1_clk_cmd_dat0_sleep &pinctrl_mmc1_dat1_3_sleep>;
				status = "okay";
				slot@0 {
					reg = <0>;
					bus-width = <4>;
					cd-gpios = <&pioD 18 GPIO_ACTIVE_HIGH>;
				};
			};

			adc0: adc@f8018000 {
				pinctrl-names = "default", "sleep";
				pinctrl-0 = <
					&pinctrl_adc0_adtrg
					&pinctrl_adc0_ad0
					&pinctrl_adc0_ad1
					&pinctrl_adc0_ad2
					&pinctrl_adc0_ad3
					&pinctrl_adc0_ad4
					>;
				pinctrl-1 = <
					&pinctrl_adc0_adtrg_sleep
					&pinctrl_adc0_ad0_sleep
					&pinctrl_adc0_ad1_sleep
					&pinctrl_adc0_ad2_sleep
					&pinctrl_adc0_ad3_sleep
					&pinctrl_adc0_ad4_sleep
					>;
				status = "okay";
			};

			lcd_bus@f0030000 {
				pinctrl-names = "default", "sleep";
				pinctrl-1 = <&pinctrl_lcd_sleep>;
			};

			macb0: ethernet@f0028000 {
				pinctrl-names = "default", "sleep";
				pinctrl-1 = <&pinctrl_macb0_data_rgmii_sleep &pinctrl_macb0_signal_rgmii_sleep>;
			};

			macb1: ethernet@f802c000 {
				phy-mode = "rmii";
				pinctrl-names = "default", "sleep";
				pinctrl-1 = <&pinctrl_macb1_rmii_sleep>;
			};

			pinctrl@fffff200 {
				/* shared pinctrl settings */
				adc0 {
					pinctrl_adc0_adtrg_sleep: adc0_adtrg_1 {
						atmel,pins =
							<AT91_PIOD 19 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD19 GPIO output 0 */
					};
					pinctrl_adc0_ad0_sleep: adc0_ad0_1 {
						atmel,pins =
							<AT91_PIOD 20 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD20 GPIO output 0 */
					};
					pinctrl_adc0_ad1_sleep: adc0_ad1_1 {
						atmel,pins =
							<AT91_PIOD 21 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD21 GPIO output 0 */
					};
					pinctrl_adc0_ad2_sleep: adc0_ad2_1 {
						atmel,pins =
							<AT91_PIOD 22 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD22 GPIO output 0 */
					};
					pinctrl_adc0_ad3_sleep: adc0_ad3_1 {
						atmel,pins =
							<AT91_PIOD 23 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD23 GPIO output 0 */
					};
					pinctrl_adc0_ad4_sleep: adc0_ad4_1 {
						atmel,pins =
							<AT91_PIOD 24 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD24 GPIO output 0 */
					};
				};

				can0 {
					pinctrl_can0_rx_tx_sleep: can0_rx_tx-1 {
						atmel,pins =
							<AT91_PIOD 14 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PD14 GPIO output 0 */
							 AT91_PIOD 15 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD15 GPIO output 0 */
					};
				};

				can1 {
					pinctrl_can1_rx_tx_sleep: can1_rx_tx-1 {
						atmel,pins =
							<AT91_PIOB 14 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB14 GPIO output 0 */
							 AT91_PIOB 15 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PB15 GPIO output 0 */
					};
				};

				mmc0 {
					pinctrl_mmc0_clk_cmd_dat0_sleep: mmc0_clk_cmd_dat0_1 {
						atmel,pins =
							<AT91_PIOD 9 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PD9 GPIO output 0 */
							 AT91_PIOD 0 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PD0 GPIO Input with pullup */
							 AT91_PIOD 1 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP>;	/* PD1 GPIO Input with pullup */
					};
					pinctrl_mmc0_dat1_3_sleep: mmc0_dat1_3_1 {
						atmel,pins =
							<AT91_PIOD 2 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PD2 GPIO Input with pullup */
							 AT91_PIOD 3 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PD3 GPIO Input with pullup */
							 AT91_PIOD 4 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP>;	/* PD4 GPIO Input with pullup */
					};
					pinctrl_mmc0_dat4_7_sleep: mmc0_dat4_7_1 {
						atmel,pins =
							<AT91_PIOD 5 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PD5 GPIO Input with pullup */
							 AT91_PIOD 6 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PD6 GPIO Input with pullup */
							 AT91_PIOD 7 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PD7 GPIO Input with pullup */
							 AT91_PIOD 8 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP>;	/* PD8 GPIO Input with pullup */
					};
				};

				mmc1 {
					pinctrl_mmc1_clk_cmd_dat0_sleep: mmc1_clk_cmd_dat0_1 {
						atmel,pins =
							<AT91_PIOB 24 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB24 GPIO output 0 */
							 AT91_PIOB 19 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PB19 GPIO Input with pullup */
							 AT91_PIOB 20 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP>;	/* PB20 GPIO Input with pullup */
					};
					pinctrl_mmc1_dat1_3_sleep: mmc1_dat1_3_1 {
						atmel,pins =
							<AT91_PIOB 21 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PB21 GPIO Input with pullup */
							 AT91_PIOB 22 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PB22 GPIO Input with pullup */
							 AT91_PIOB 23 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP>;	/* PB23 GPIO Input with pullup */
					};
				};

				i2c0 {
					pinctrl_i2c0_sleep: i2c0-1 {
						atmel,pins =
							<AT91_PIOA 30 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT1	/* PA30 GPIO output 1 */
							 AT91_PIOA 31 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT1>;	/* PA31 GPIO output 1 */
					};
				};

				spi0 {
					pinctrl_spi0_sleep: spi0-1 {
						atmel,pins =
							<AT91_PIOD 10 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PD10 GPIO output 0 */
							 AT91_PIOD 11 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PD11 GPIO output 0 */
							 AT91_PIOD 12 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD12 GPIO output 0 */
					};
				};

				ssc0 {
					pinctrl_ssc0_tx_sleep: ssc0_tx-1 {
						atmel,pins =
							<AT91_PIOC 16 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC16 GPIO output 0 */
							 AT91_PIOC 17 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC17 GPIO output 0 */
							 AT91_PIOC 18 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PC18 GPIO output 0 */
					};

					pinctrl_ssc0_rx_sleep: ssc0_rx-1 {
						atmel,pins =
							<AT91_PIOC 19 AT91_PERIPH_GPIO AT91_PINCTRL_NONE	/* PC19 GPIO Input */
							 AT91_PIOC 20 AT91_PERIPH_GPIO AT91_PINCTRL_NONE	/* PC20 GPIO Input */
							 AT91_PIOC 21 AT91_PERIPH_GPIO AT91_PINCTRL_NONE>;	/* PC21 GPIO Input */
					};
				};

				isi {
					pinctrl_isi_sleep: isi-1 {
						atmel,pins =
							<AT91_PIOA 16 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PA16 GPIO Input with pullup */
							 AT91_PIOA 17 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PA17 GPIO Input with pullup */
							 AT91_PIOA 18 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PA18 GPIO Input with pullup */
							 AT91_PIOA 19 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PA19 GPIO Input with pullup */
							 AT91_PIOA 20 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PA20 GPIO Input with pullup */
							 AT91_PIOA 21 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PA21 GPIO Input with pullup */
							 AT91_PIOA 22 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PA22 GPIO Input with pullup */
							 AT91_PIOA 23 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP	/* PA23 GPIO Input with pullup */
							 AT91_PIOC 30 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC30 GPIO output 0 */
							 AT91_PIOA 31 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT1	/* PA31 GPIO output 1 */
							 AT91_PIOA 30 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT1	/* PA30 GPIO output 1 */
							 AT91_PIOC 29 AT91_PERIPH_GPIO AT91_PINCTRL_NONE	/* PC29 GPIO Input */
							 AT91_PIOC 28 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PC28 GPIO output 0 */
					};
					pinctrl_isi_pck_as_mck_sleep: isi_pck_as_mck-1 {
						atmel,pins =
							<AT91_PIOD 31 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PD31 periph B ISI_MCK */
					};
				};

				usart1 {
					pinctrl_usart1_sleep: usart1-1 {
						atmel,pins =
							<AT91_PIOB 28 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB28 GPIO output 0 */
							 AT91_PIOB 29 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PB29 GPIO output 0 */
					};

					pinctrl_usart1_rts_cts_sleep: usart1_rts_cts-1 {
						atmel,pins =
							<AT91_PIOB 26 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB26 GPIO output 0 */
							 AT91_PIOB 27 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PB27 GPIO output 0 */
					};
				};

				lcd {
					pinctrl_lcd_sleep: lcd-1 {
						atmel,pins =
							<AT91_PIOA 24 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA24 GPIO output 0 */
							 AT91_PIOA 26 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA26 GPIO output 0 */
							 AT91_PIOA 27 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA27 GPIO output 0 */
							 AT91_PIOA 25 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA25 GPIO output 0 */
							 AT91_PIOA 29 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA29 GPIO output 0*/
							 AT91_PIOA 28 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA28 GPIO output 0 */
							 AT91_PIOA 0 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA0 GPIO output 0 */
							 AT91_PIOA 1 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA1 GPIO output 0 */
							 AT91_PIOA 2 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA2 GPIO output 0 */
							 AT91_PIOA 3 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA3 GPIO output 0 */
							 AT91_PIOA 4 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA4 GPIO output 0 */
							 AT91_PIOA 5 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA5 GPIO output 0 */
							 AT91_PIOA 6 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA6 GPIO output 0 */
							 AT91_PIOA 7 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA7 GPIO output 0 */
							 AT91_PIOA 8 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA8 GPIO output 0 */
							 AT91_PIOA 9 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA9 GPIO output 0 */
							 AT91_PIOA 10 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA10 GPIO output 0 */
							 AT91_PIOA 11 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA11 GPIO output 0 */
							 AT91_PIOA 12 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA12 GPIO output 0 */
							 AT91_PIOA 13 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA13 GPIO output 0 */
							 AT91_PIOA 14 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA14 GPIO output 0 */
							 AT91_PIOA 15 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PA15 GPIO output 0 */
							 AT91_PIOC 14 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC14 GPIO output 0 */
							 AT91_PIOC 13 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC13 GPIO output 0 */
							 AT91_PIOC 12 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC12 GPIO output 0 */
							 AT91_PIOC 11 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC11 GPIO output 0 */
							 AT91_PIOC 10 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC10 GPIO output 0 */
							 AT91_PIOC 15 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC15 GPIO output 0 */
							 AT91_PIOE 28 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PE28 PIO output 0 */
					};
				};

				macb0 {
					pinctrl_macb0_data_rgmii_sleep: macb0_data_rgmii_1 {
						atmel,pins =
							<AT91_PIOB 0 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB0 GPIO output 0 */
							 AT91_PIOB 1 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB1 GPIO output 0 */
							 AT91_PIOB 2 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB2 GPIO output 0 */
							 AT91_PIOB 3 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB3 GPIO output 0 */
							 AT91_PIOB 4 AT91_PERIPH_GPIO AT91_PINCTRL_NONE		/* PB4 GPIO input default */
							 AT91_PIOB 5 AT91_PERIPH_GPIO AT91_PINCTRL_NONE		/* PB5 GPIO input default */
							 AT91_PIOB 6 AT91_PERIPH_GPIO AT91_PINCTRL_NONE		/* PB6 GPIO input default */
							 AT91_PIOB 7 AT91_PERIPH_GPIO AT91_PINCTRL_NONE>;	/* PB7 GPIO input default */
					};

					pinctrl_macb0_signal_rgmii_sleep: macb0_signal_rgmii_1 {
						atmel,pins =
							<AT91_PIOB 8 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB8 GPIO output 0 */
							 AT91_PIOB 9 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB9 GPIO output 0 */
							 AT91_PIOB 11 AT91_PERIPH_GPIO AT91_PINCTRL_NONE	/* PB11 GPIO input default */
							 AT91_PIOB 13 AT91_PERIPH_GPIO AT91_PINCTRL_NONE	/* PB13 GPIO input default */
							 AT91_PIOB 16 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB16 GPIO output 0 */
							 AT91_PIOB 17 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PB17 GPIO output 0 */
							 AT91_PIOB 18 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0>;	/* PB18 GPIO output 0 */
					};
				};

				macb1 {
					pinctrl_macb1_rmii_sleep: macb1_rmii-1 {
						atmel,pins =
							<AT91_PIOC 0 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC0 GPIO output 0 */
							 AT91_PIOC 1 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC1 GPIO output 0 */
							 AT91_PIOC 2 AT91_PERIPH_GPIO AT91_PINCTRL_NONE		/* PC2 GPIO input default */
							 AT91_PIOC 3 AT91_PERIPH_GPIO AT91_PINCTRL_NONE		/* PC3 GPIO input default */
							 AT91_PIOC 4 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT0	/* PC4 GPIO output 0 */
							 AT91_PIOC 5 AT91_PERIPH_GPIO AT91_PINCTRL_NONE		/* PC5 GPIO input default */
							 AT91_PIOC 6 AT91_PERIPH_GPIO AT91_PINCTRL_NONE		/* PC6 GPIO input default */
							 AT91_PIOC 7 AT91_PERIPH_GPIO AT91_PINCTRL_NONE		/* PC7 GPIO input default */
							 AT91_PIOC 8 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT1	/* PC8 GPIO output 1 */
							 AT91_PIOC 9 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT1>;	/* PC9 GPIO output 1 */
					};
				};

				board {
					pinctrl_mmc0_cd: mmc0_cd {
						atmel,pins =
							<AT91_PIOD 17 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP_DEGLITCH>; /* PD17 GPIO with pullup deglitch */
					};

					pinctrl_mmc1_cd: mmc1_cd {
						atmel,pins =
							<AT91_PIOD 18 AT91_PERIPH_GPIO AT91_PINCTRL_PULL_UP_DEGLITCH>; /* PD18 GPIO with pullup deglitch */
					};

					pinctrl_pck0_as_audio_mck: pck0_as_audio_mck {
						atmel,pins =
							<AT91_PIOD 30 AT91_PERIPH_B AT91_PINCTRL_NONE>;	/* PD30 periph B */
					};

					pinctrl_isi_reset: isi_reset-0 {
						atmel,pins =
							<AT91_PIOE 24 AT91_PERIPH_GPIO AT91_PINCTRL_NONE>;   /* PE24 gpio */
					};

					pinctrl_isi_reset_sleep: isi_reset-1 {
						atmel,pins =
							<AT91_PIOE 24 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT1>;   /* PE24 GPIO output 1 */
					};

					pinctrl_isi_power: isi_power-0 {
						atmel,pins =
							<AT91_PIOE 29 AT91_PERIPH_GPIO AT91_PINCTRL_NONE>; /* PE29 gpio */
					};

					pinctrl_isi_power_sleep: isi_power-1 {
						atmel,pins =
							<AT91_PIOE 29 AT91_PERIPH_GPIO AT91_PINCTRL_OUTPUT1>; /* PE29 GPIO output 1 */
					};

					pinctrl_usba_vbus: usba_vbus {
						atmel,pins =
							<AT91_PIOD 29 AT91_PERIPH_GPIO AT91_PINCTRL_DEGLITCH>; /* PD29 GPIO with deglitch */
					};
				};
			};

			dbgu: serial@ffffee00 {
				status = "okay";
			};

			watchdog@fffffe40 {
				status = "okay";
			};
		};

		usb0: gadget@00500000 {
			atmel,vbus-gpio = <&pioD 29 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&pinctrl_usba_vbus>;
			status = "okay";
		};

		usb1: ohci@00600000 {
			num-ports = <3>;
			atmel,vbus-gpio = <&pioD 25 GPIO_ACTIVE_HIGH
					   &pioD 26 GPIO_ACTIVE_LOW
					   &pioD 27 GPIO_ACTIVE_LOW
					  >;
			status = "okay";
		};

		usb2: ehci@00700000 {
			status = "okay";
		};
	};

	sound {
		compatible = "atmel,asoc-wm8904";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_pck0_as_audio_mck>;

		atmel,model = "wm8904 @ SAMA5D3EK";
		atmel,audio-routing =
			"Headphone Jack", "HPOUTL",
			"Headphone Jack", "HPOUTR",
			"IN2L", "Line In Jack",
			"IN2R", "Line In Jack",
			"Mic", "MICBIAS",
			"IN1L", "Mic";

		atmel,ssc-controller = <&ssc0>;
		atmel,audio-codec = <&wm8904>;
		status = "disabled";
	};

	wm8904_3v3_power: wm8904_3v3_reg {
		compatible = "regulator-fixed";
		regulator-name = "wm8904_3v3";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	wm8904_1v8_power: wm8904_1v8_reg {
		compatible = "regulator-fixed";
		regulator-name = "wm8904_1v8";
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;

		pb_user1 {
			label = "pb_user1";
			gpios = <&pioE 27 GPIO_ACTIVE_HIGH>;
			linux,code = <0x100>;
			gpio-key,wakeup;
		};
	};
};
